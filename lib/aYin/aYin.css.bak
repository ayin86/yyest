.loading{position: fixed; top:0; right:0; bottom:0; left:0; z-index: 999; background-color:rgba(0,0,0,0.3);}
.loading .loading-pos{width: 400px;   position: absolute; background-color: rgba(255,255,255,.8); border-radius:10px; left: 50%; top: 20%;  margin-left:-200px;  }
.loading .loading-pos .loading-img{background-image: url(images/loading.gif);  border-radius:45px; overflow: hidden; background-repeat: no-repeat; background-position:center center;}
.loading .loading-pos .loading-img{width: 90px; height:90px;  margin: 25px auto; background-color: #fff; }
.loading .loading-pos .loading-desc{font-size: 1.3em; color:#2caedc; text-align: center; margin:25px 0;}
.loading .loading-pos .loading-percent{display: none; font-size: 1.3em; color:#2caedc; text-align: center; margin:25px 0;}
.loading .loading-pos .loading-ctrl{display:none; text-align: center; margin:25px 0;}
.loading .loading-pos .loading-ctrl .btn{margin:0 10px;}
.loading-area{position: absolute;z-index: 9;}

.popupAlert{ }
.popupAlert-ani-full{position: fixed;top:0; right:0; bottom:0; left:0; z-index: 999; animation: alert-full 1s infinite ease-in-out;}
.popupAlert-ani-small{animation: alert-small 1s infinite ease-in-out;}
.popupAlert .PA-pos .PA-icon{ position: absolute; left:10px; top:13px;}
.popupAlert .PA-pos .PA-desc{margin:0 0 0 25px;}
.popupAlert .PA-pos .PA-cancel{background-color: #999;width:24px; height:24px; border-radius:12px; text-align: center; padding:4px 0 0 0; position: absolute; right:-10px; top:-10px;}
.popupAlert .PA-pos .PA-cancel:before{content:"\f00d";}

.popupAlert .PA-pos{display: none; z-index: 999; position: fixed; background-color: rgba(0,0,0,.7); color: #fff; font-size: 1.2em;padding:10px; 
right:30px; max-width: 500px; bottom:30px; border-radius:5px; border: 1px solid #333; box-shadow:3px 3px 10px rgba(0,0,0,.2); }
.popupAlert .PA-pos-success{background-color:#dff0d8; color:#3c763d;border-color: #d6e9c6}
.popupAlert .PA-pos-info{background-color:#d9edf7;color: #31708f;border-color: #bce8f1}
.popupAlert .PA-pos-warning{background-color: #fcf8e3;color: #8a6d3b;border-color: #faebcc;}
.popupAlert .PA-pos-danger{background-color:#f2dede;color: #a94442;border-color: #ebccd1}
.popupAlert .PA-pos-dark{background-color: rgba(0,0,0,.7);}

.popupAlert .PA-pos-success .PA-cancel{background-color:#d6e9c6; color:#3c763d;}
.popupAlert .PA-pos-info .PA-cancel{background-color: #bce8f1;color: #31708f;}
.popupAlert .PA-pos-warning .PA-cancel{background-color: #faebcc;color: #8a6d3b;}
.popupAlert .PA-pos-danger .PA-cancel{background-color: #ebccd1;color: #a94442;}
.popupAlert .PA-pos-dark .PA-cancel{background-color: rgba(0,0,0,.8);}


.empty-content{position: absolute; left:0; right:0; top:0; bottom:0;}
.empty-content .ec-inner{content:"\f00d";color:#999; width:200px; height:160px; position: absolute; top:50%; left:50%; margin-left:-100px; margin-top:-80px; text-align: center; }
.empty-content .ec-inner .ec-icon{font-size: 8em; color:#ccc; margin:0 0 10px 0;}
.empty-content .ec-inner .ec-icon:before{content:"\f24a";}
.empty-content .ec-inner .ec-desc{font-size: 1.5em;}


@keyframes buzz-out {
  0% {transform: translateX(3px) rotate(1deg);}
  50% {transform: translateX(-3px) rotate(-1deg);}
  100% {transform: translateX(3px) rotate(1deg);}
}
@keyframes alert-full
{
0% { box-shadow:0 0 100px rgba(255,0,0,0) inset;}
50% { box-shadow:0 0 100px rgba(255,0,0,.8) inset;}
100% { box-shadow:0 0 100px rgba(255,0,0,0) inset;}
}
@keyframes alert-small
{
0% { box-shadow:0 0 10px rgba(255,0,0,0) ;}
50% { box-shadow:0 0 10px rgba(255,0,0,.8) ;}
100% { box-shadow:0 0 10px rgba(255,0,0,0) ;}
}


